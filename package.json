{
  "name": "n8n-nodes-telegram-grampro",
  "version": "3.0.1",
  "description": "Telegram GramPro production nodes for n8n",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node esbuild.config.mjs && node copy-assets.mjs",
    "test": "npm run lint && npm run build",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "dev": "node esbuild.config.mjs --watch",
    "n8n-scan": "npx @n8n/scan-community-package .",
    "prepare": "npm run build"
  },
  "keywords": [
    "n8n",
    "n8n-nodes",
    "telegram",
    "telegram-api",
    "telegram-mtproto",
    "telegram-grampro",
    "gramjs",
    "automation",
    "workflow",
    "n8n-community-node-package"
  ],
  "author": {
    "name": "sadiakant",
    "email": "krushnakantsadiya@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadiakant/n8n-nodes-telegram-grampro.git"
  },
  "homepage": "https://github.com/sadiakant/n8n-nodes-telegram-grampro#readme",
  "bugs": {
    "url": "https://github.com/sadiakant/n8n-nodes-telegram-grampro/issues"
  },
  "dependencies": {
    "n8n-workflow": "*"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@n8n/node-cli": "^0.20.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.19.33",
    "esbuild": "^0.25.0",
    "eslint": "^9.10.0",
    "globals": "^15.0.0",
    "telegram": "^2.26.22",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.0.0"
  },
  "n8n": {
    "nodes": [
      "dist/nodes/TelegramMtproto.node.js",
      "dist/nodes/TelegramTrigger.node.js"
    ],
    "credentials": [
      "dist/credentials/TelegramApi.credentials.js"
    ]
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
